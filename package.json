{
  "name": "@vase/redis-on-workers",
  "version": "0.3.4",
  "description": "Redis client for Cloudflare Workers, patched and maintained by Vase for improved ACL compatibility and internal workflow standards",
  "scripts": {
    "build": "tsup",
    "test": "tsx --test"
  },
  "main": "dist/index.mjs",
  "files": [
    "dist",
    "package.json",
    "README.md",
