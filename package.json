  "author": {
    "name": "Luvesh",
    "email": "luvesh.sadhwani@vase.ai"
  },
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241011.0",
    "@types/node": "^22.7.5",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "@arrowood.dev/socket": "^0.2.0"
  }
